@page
@model IndexModel
@{
	ViewData["Title"] = "Home Page";
}

<div class="text-center">
	<h1 class="display-4">Welcome</h1>
	<div class="d-flex bg-white text-black justify-content-center">
		<div>
			<h1>Decrypted Data</h1>
			<p>Username: @Model.CurrentUser.Username</p>
			<p>Email: @Model.CurrentUser.Email</p>
			<p>First Name: @Model.CurrentUser.FirstName</p>
			<p>Last Name: @Model.CurrentUser.LastName</p>
			<p>NRIC: @Model.CurrentUser.NRIC</p>
			<p>Gender: @Model.CurrentUser.Gender</p>
			<p>WhoamI: @Model.CurrentUser.WhoamI</p>
			<p>Date of Birth: @Model.CurrentUser.DateOfBirth</p>
		</div>
	</div>
	<div class="d-flex bg-white text-black justify-content-center">
		<div>
			<h1>Encrypted Data</h1>
			<p>Username: @Model.EncryptedUser.Username</p>
			<p>Email: @Model.EncryptedUser.Email</p>
			<p>First Name: @Model.EncryptedUser.FirstName</p>
			<p>Last Name: @Model.EncryptedUser.LastName</p>
			<p>NRIC: @Model.EncryptedUser.NRIC</p>
			<p>Gender: @Model.EncryptedUser.Gender</p>
			<p>WhoamI: @Model.EncryptedUser.WhoamI</p>
			<p>Date of Birth: @Model.EncryptedUser.DateOfBirth</p>
		</div>
	</div>
</div>
